<UserControl x:Class="Freed.Api.Monitor.UserControlTmp.GroupTotaltStatisticsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Freed.Api.Monitor.UserControlTmp"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Loaded="UserControl_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Border Grid.Row="0">
            <ScrollViewer VerticalScrollBarVisibility="Disabled" Grid.Row="0">
                <UniformGrid Name="RepertoryGrid"  />
            </ScrollViewer>
        </Border>
    </Grid>
</UserControl>
